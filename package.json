		"base64-stream": "^1.0.0",
		"get-stream": "^6.0.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"remix-auth": "^3.2.2",
		"remix-auth-socials": "^1.0.2"
	},
	"devDependencies": {
		"@remix-run/dev": "^1.7.0",
		"@remix-run/eslint-config": "^1.7.0",
		"@types/base64-stream": "^1.0.2",
		"@types/node": "^18.7.14",
		"@types/react": "^18.0.15",
		"@types/react-dom": "^18.0.6",
		"eslint": "^8.20.0",
		"npm-run-all": "^4.1.5",
